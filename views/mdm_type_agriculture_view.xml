<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="type_agriculture_view_form" model="ir.ui.view">
            <field name="name">ประเภทการทำการเกษตร</field>
            <field name="model">mdm.type.agriculture</field>
            <field name="arch" type="xml">
                <form string="ประเภทการทำการเกษตร">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="type_agriculture_view_tree" model="ir.ui.view">
            <field name="name">ประเภทการทำการเกษตร</field>
            <field name="model">mdm.type.agriculture</field>
            <field name="arch" type="xml">
                <tree string="ประเภทการทำการเกษตร">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="type_agriculture_action" model="ir.actions.act_window">
            <field name="name">ประเภทการทำการเกษตร</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mdm.type.agriculture</field>
            <field name="view_mode">tree,form</field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="type_agriculture_menu" name="ประเภทการทำการเกษตร" parent="main_menu_information" action="type_agriculture_action"
                  sequence="5"/>
    </data>
</odoo>